debugwrapper = false;(function(){
var callbacks = {invoke:[],completed:[],postcompleted:[],failed:[]};
var onceCallbacks = {invoke:[],completed:[],postcompleted:[],failed:[]};commandqueue = {
blocked: false,queue: [],startcommand: function() {commandqueue.blocked = true;},
isblocked: function() {return commandqueue.blocked;},push: function(ajaxOptions) {
commandqueue.queue.push(ajaxOptions);},process: function() {if (commandqueue.queue.length > 0) {
ajaxOptions = commandqueue.queue.shift();$.ajax(ajaxOptions);}
commandqueue.blocked = commandqueue.queue.length > 0;},reset: function() {
commandqueue.blocked = false;commandqueue.queue = [];}};var set_sync = false;var sync_output = null;
Blocked = function(method) {var args = Array.prototype.slice.call(arguments);args.shift();
set_sync = true;method.apply(this,args);set_sync = false;var output = sync_output;
sync_output = null;return output;};var runCallbacks = function(cbkey, method, results) {var i;
var fnc;var cbList;if (callbacks[cbkey]) {cbList = callbacks[cbkey];
for (i = 0; i < cbList.length; ++i) {fnc = cbList[i];try {fnc(method, results);} catch (e) {
if (debugwrapper) {OnErrorMsgDialog(e.message + "<br/>Error from: " + cbkey);} else {throw e;}}}}
if (onceCallbacks[cbkey]) {cbList = onceCallbacks[cbkey];for (i = 0; i < cbList.length; ++i) {
fnc = cbList[i];try {fnc(method, results);} catch (e2) {if (debugwrapper) {
OnErrorMsgDialog(e2.message + "<br/>Error from: " + cbkey);} else {throw e2;}}}
onceCallbacks[cbkey] = [];}};function priv_get_client_state() {return "none";}PicassoWebServices = {
addInvokingRequest: function(fnc, runOnce) {if (typeof fnc === 'function') {if (runOnce === true) {
onceCallbacks.invoke.push(fnc);} else {callbacks.invoke.push(fnc);}}},
addCompletedRequest: function(fnc, runOnce) {if (typeof fnc === 'function') {if (runOnce === true) {
onceCallbacks.completed.push(fnc);} else {callbacks.completed.push(fnc);}}},
addPostCompletedRequest: function(fnc, runOnce) {if (typeof fnc === 'function') {
if (runOnce === true) {onceCallbacks.postcompleted.push(fnc);} else {
callbacks.postcompleted.push(fnc);}}},addFailedRequest: function(fnc, runOnce) {
if (typeof fnc === 'function') {if (runOnce === true) {onceCallbacks.failed.push(fnc);} else {
callbacks.failed.push(fnc);}}},
_invoke : function(path, command, parameters, allowAsync, succeededCallback, failedCallback, userContext, filters) {
var ajaxOptions = {type : "POST",
url : path + "?Method=" + command + (window.$P.app && window.getCurrentSiteGUID && window.getCurrentSiteGUID() ? "&siteGuid=" + window.getCurrentSiteGUID() : "") +
(window.$P.app && window.$P.app.currentContextID? "&docGuid=" + window.$P.app.currentContextID : "") +
(window.$P.app && window.$P.app.siteType ? "&siteType=" + window.$P.app.siteType : "") +
(window.$P.isContributor ? "&iscontrib=true" : "") +(window.$P.isBuilder ? "&isbuilder=true" : "") +
(window.csrfToken ? "&_CONFIRMATIONTOKEN=" + window.csrfToken : ""),
contentType : "application/json; charset=utf-8",dataType : "json",async: true && !set_sync,
data : JSON.stringify({params : parameters, filter : filters}),tryCount : 0,retryLimit : 1,
success : function(msg) {try {
if (msg && msg.ErrorCode === '401' && msg.UpdatedCsrfToken && msg.CsrfFailureReason === 'S') {
this.tryCount++;if (this.tryCount <= this.retryLimit) {window.csrfToken = msg.UpdatedCsrfToken;
this.url = this.url.slice(0, this.url.indexOf('_CONFIRMATIONTOKEN')) + '_CONFIRMATIONTOKEN=' + msg.UpdatedCsrfToken;
$.ajax(this);return;}}runCallbacks('completed', command, msg);if (succeededCallback) {
succeededCallback.apply(this, [ msg ]);}runCallbacks('postcompleted', command, msg);
if (!allowAsync) {commandqueue.process();}} catch (e) {OnErrorMsgDialog(e.message);
if (!allowAsync) {commandqueue.reset();}if (!e.dontPropagate) {throw e;}}},error: function(msg) {
runCallbacks('failed', command, msg);if (failedCallback) {
failedCallback.apply(this, [ command, msg ]);}if (!allowAsync) {commandqueue.reset();}}};
runCallbacks('invoke', command);if (!allowAsync && commandqueue.isblocked()) {
commandqueue.push(ajaxOptions);return;}$.ajax(ajaxOptions);if (!allowAsync) {
commandqueue.startcommand();}},_get_path: function() {return "/sfsites/sfservice";},
isBlocked: function() {return commandqueue.isblocked();},
